{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser chrome --headless",
    "dev:test": "vite --host 127.0.0.1",
    "cy:run:with-dev": "concurrently -k -n vite,cy \"npm run dev\" \"wait-on http://localhost:5173 && npm run cy:run\"",
